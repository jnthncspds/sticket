{% extends 'controlpanel.html.twig' %}
{% block body %}
<h1 style="margin-top: 20px"> Lista de Usuarios Registrados </h1>
{% block table %}
<table class="table">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Nombre de usuario</th>
      <th scope="col">Email</th>
    </tr>
  </thead>
  <tbody>
    {% for u in usuario  %}
    <tr>
      <th scope="row">{{u.id}}</th>
      <td>{{u.username}}</td>
      <td>{{u.email}}</td>
    </tr>
      {% endfor %}
  </tbody>
</table>
{% endblock %}
{% endblock %}
